<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Course Types</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body class="container my-5">
  <h1>Course Types</h1>
  <ul class="list-group">
    <% courseTypes.forEach(type => { %>
      <li class="list-group-item">
        <a href="/semester/<%= semesterId %>/type/<%= type.id %>/course-names"><%= type.name %></a>
      </li>
    <% }) %>
  </ul>
  <a href="/courses" class="btn btn-link mt-3">Back to Courses</a>
</body>
</html>
