<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Courses</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body class="container my-5">
  <h1>Courses</h1>
  <ul class="list-group">
    <% courseNames.forEach(course => { %>
      <li class="list-group-item">
        <a href="/course-name/<%= course.id %>/materials"><%= course.name %></a>
      </li>
    <% }) %>
  </ul>
  <a href="/courses" class="btn btn-link mt-3">Back to Courses</a>
</body>
</html>
